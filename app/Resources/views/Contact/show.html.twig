{% extends 'base.html.twig' %}

{% block title %}app:Resources:Contact:show{% endblock %}

{% block body %}
<p>
    <p>Id: {{contact.id}}</p>
    <p>Name: {{contact.name}}</p>
    <p>Surname: {{contact.surname}}</p>
    <p>Description: {{contact.description}}</p>

    <a href="{{ path('app_contact_modify', {'id': contact.id}) }}">Edytuj kontakt</a>
</p>
{% endblock %}

